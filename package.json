{
  "name": "livsey-finance-library",
  "version": "1.0.2",
  "description": "a simple financial library with most important ratios for investors/traders/etc",
  "type": "module",
  "main": "dist/index.js",
  "scripts": {
    "start": "node --no-warnings=ExperimentalWarning --loader ts-node/esm ./index.ts",
    "build": "tsc"
  },
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.js"
    },
    "./node_modules/livsey-finance-library/": {
      "import": "./dist/index.js",
      "require": "./dist/index.js"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/livesey-finance/livsey-finance-library.git"
  },
  "files": [
    "index.js",
    "index.ts",
    "tsconfig.json",
    "dist/"
  ],
  "author": "hhuziuk",
  "license": "MIT",
  "dependencies": {
    "@types/big.js": "^6.2.2",
    "big.js": "^6.2.1",
    "typescript": "^5.5.3"
  },
  "devDependencies": {
    "ts-node": "^10.9.2"
  }
}
